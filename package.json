{
  "name": "time-tracking",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "cross-env eslint '{src,pages}/**/*.{ts,tsx}'",
    "lint:fix": "cross-env eslint --fix '{src,pages}/**/*.{ts,tsx}'",
    "prettier:check": "cross-env prettier --check '{src,pages}/**/*.{ts,tsx}'",
    "prettier:fix": "cross-env prettier --write '{src,pages}/**/*.{ts,tsx}'",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "type-check": "tsc --noEmit --incremental false",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.0",
    "@ant-design/icons": "^5.5.1",
    "@reduxjs/toolkit": "^2.3.0",
    "antd": "^5.22.1",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "lodash": "^4.17.21",
    "next": "15.0.2",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-redux": "^9.1.2",
    "react-webcam": "^7.2.0",
    "redux": "^5.0.1",
    "redux-injectors": "^2.1.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint",
      "eslint --fix",
      "prettier --write"
    ]
  }
}
